{
  "name": "forum_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e env/.env.dev next dev --turbopack",
    "build": "dotenv -e env/.env.dev next build",
    "start": "dotenv -e env/.env.dev next start",
    "lint": "next lint",
    "prisma:generate": "dotenv -e env/.env.dev prisma generate",
    "prisma:migrate": "dotenv -e env/.env.dev prisma migrate dev",
    "prisma:studio": "dotenv -e env/.env.dev prisma studio",
    "prisma:seed": "dotenv -e env/.env.dev prisma db seed",
    "prisma:reset": "dotenv -e env/.env.dev prisma migrate reset"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} database/seed.ts",
    "schema": "database/schema"
  },
  "dependencies": {
    "@material-symbols/font-400": "^0.31.2",
    "@mdxeditor/editor": "^3.30.1",
    "@prisma/client": "^6.6.0",
    "@tailwindcss/postcss": "^4.1.4",
    "dayjs": "^1.11.13",
    "dompurify": "^3.2.5",
    "dotenv": "^16.5.0",
    "escape-string-regexp": "^5.0.0",
    "jsdom": "^26.1.0",
    "minio": "^8.0.5",
    "next": "15.3.1",
    "next-auth": "^4.24.11",
    "next-mdx-remote": "^5.0.0",
    "prisma": "^6.6.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "remark-gfm": "^4.0.1",
    "sass": "^1.86.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/dompurify": "^3.2.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.0",
    "eslint-config-next": "15.3.1",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^4.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
